{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:PutObject",
          "s3:DeleteObject",
          "s3:ListBucket"
        ],
        "Resource": [
          "arn:aws:s3:::aws_s3_bucket.my_s3_bucket.bucket/*",
          "arn:aws:s3:::aws_s3_bucket.my_s3_bucket.bucket"
        ]
      }
    ]
  }
  